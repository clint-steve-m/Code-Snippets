<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>IDECodeSnippetCompletionPrefix</key>
	<string>Snippet: String: indexOf() character &amp; lastIndexOf() in a String</string>
	<key>IDECodeSnippetCompletionScopes</key>
	<array>
		<string>TopLevel</string>
	</array>
	<key>IDECodeSnippetContents</key>
	<string>extension String {
    func indexOf(_ input: String,
                 options: String.CompareOptions = .literal) -&gt; String.Index? {
        return self.range(of: input, options: options)?.lowerBound
    }
    
    func lastIndexOf(_ input: String) -&gt; String.Index? {
        return indexOf(input, options: .backwards)
    }
}</string>
	<key>IDECodeSnippetIdentifier</key>
	<string>54D4E231-F33E-45E3-B7A9-B4CF339D7287</string>
	<key>IDECodeSnippetLanguage</key>
	<string>Xcode.SourceCodeLanguage.Swift</string>
	<key>IDECodeSnippetPlatformFamily</key>
	<string>iphoneos</string>
	<key>IDECodeSnippetSummary</key>
	<string>indexOf() character &amp; lastIndexOf() in a String</string>
	<key>IDECodeSnippetTitle</key>
	<string>String: indexOf() character &amp; lastIndexOf() in a String</string>
	<key>IDECodeSnippetUserSnippet</key>
	<true/>
	<key>IDECodeSnippetVersion</key>
	<integer>2</integer>
</dict>
</plist>
